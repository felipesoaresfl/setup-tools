{
    "editor.fontSize": 16,
    "editor.lineHeight": 1.75,
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    //quebra linha
    "outline.icons": false,
    //retira os icones do breadcrumbs.filePath
    "telemetry.telemetryLevel": "off",
    //para de enviar informações para microsoft + privacidade
    "security.workspace.trust.untrustedFiles": "open",
    //para de exibir mensagem de segurança toda vez que abrir arquivo novo
    "editor.formatOnSave": true,
    
}